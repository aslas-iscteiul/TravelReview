<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity TravelReviewProcess.
    -->
    <changeSet id="20210401000002-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="airline_company_id"
                                 baseTableName="travel_review_process"
                                 constraintName="fk_travel_review_process__airline_company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="airline_company"/>

        <addForeignKeyConstraint baseColumnNames="accommodation_id"
                                 baseTableName="travel_review_process"
                                 constraintName="fk_travel_review_process__accommodation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="accommodation"/>

        <addForeignKeyConstraint baseColumnNames="flight_score_id"
                                 baseTableName="travel_review_process"
                                 constraintName="fk_travel_review_process__flight_score_id"
                                 referencedColumnNames="id"
                                 referencedTableName="score"/>

        <addForeignKeyConstraint baseColumnNames="booking_score_id"
                                 baseTableName="travel_review_process"
                                 constraintName="fk_travel_review_process__booking_score_id"
                                 referencedColumnNames="id"
                                 referencedTableName="score"/>

        <addForeignKeyConstraint baseColumnNames="attraction_id"
                                 baseTableName="travel_review_process"
                                 constraintName="fk_travel_review_process__attraction_id"
                                 referencedColumnNames="id"
                                 referencedTableName="attraction"/>
    </changeSet>
</databaseChangeLog>
